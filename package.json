{
	"name": "vite-vue-typescript-starter",
	"private": true,
	"version": "1.0.0-bate",
	"type": "module",
	"scripts": {
		"start": "vite --mode=3d --config config.3d.dev.ts",
		"serve": "vite --mode=2d --config config.2d.dev.ts",
		"dev": "vite",
		"test:2d": "vite --mode=test2d --config config.2d.dev.ts",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview"
	},
	"dependencies": {
		"@svgdotjs/svg.js": "^3.1.2",
		"element-plus": "^2.2.16",
		"pixi.js": "5.3.9",
		"pixi.js-legacy": "5.3.9",
		"vue": "^3.2.38"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^3.1.0",
		"typescript": "^4.6.4",
		"unplugin-auto-import": "^0.11.2",
		"unplugin-vue-components": "^0.22.4",
		"vite": "^3.1.0",
		"vue-tsc": "^0.40.7"
	}
}
